/* Label - Label de los Forms */
.sapMLabel {
	color: #666666;
	font-size: 0.875rem;
	font-family: Arial,Helvetica,sans-serif;
	font-family: Tahoma;
	font-weight: normal;
	display: inline-block;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
	cursor: text;
}

/* Label - Header superior */
.sapMIBar>.sapMBarLeft>.sapMBarChild:last-child, .sapMIBar .sapMBarPH>.sapMBarChild:last-child {
	margin-right: 0;
	/* font-weight: bold; */
	color: #F7F7F7;
	font-size: 18px;
	font-family: Arial,Helvetica,sans-serif;
}

/*!* Base del iconTab Bar*!*/
.sapMITBSelected.sapMITBFilterPositive>.sapMITBContentArrow{
	border-bottom-color: #7D398D;
}

/*!* Borde de los botones tipo Accept*!*/
span.sapMBtnInner.sapMBtnAccept{
	border-color: #73d0f5;
	color: #7D398D;
	margin-left: 4px;
}

/*!*Panel del IconTabBar*!*/
/*.sapMITH{*/
	/*background-color: #d9bdd2;*/
	/*box-shadow: none;*/
/*}*/

/* Label - Titulo de la Cabecera Forms section */
.sapUiFormM .sapUiFormContainerTitle>.sapUiFormTitle {
	height: 3rem;
	line-height: 3rem;
	color: #D6C1DB;
	color: #7E3C8F;
	font-family: Arial,Helvetica,sans-serif;
}

/* Grilla - Header de las columnas impares */
.sapUiTableCol:nth-child(even)
{
	background-color: #D6C1DB;
	color: #73d0f5;
}

/* Grilla - Header de las columnas pares */
.sapUiTableCol:nth-child(odd)
{
	background-color: #D6C1DB;
	color: white;
}

/* Grilla - Icono de los Header */
 .sapUiFormM:not(.sapUiFormEdit) .sapUiFormResGridCont>div .sapUiIcon:not(.sapMBtnIcon), .sapUiFormM:not(.sapUiFormEdit) .sapUiFormResGridCont>div .sapMCbBg {
	font-size: 16px;
	color: deeppink;
	color: black;
}

/* Grilla - Letra de los Header (Estandar)*/
.sapMObjStatusNone .sapMObjStatusText, .sapMObjStatus .sapMObjStatusIcon {
	text-shadow: none;
	/*color: aliceblue;*/
	font-weight: normal;
	font-family: Arial,Helvetica,sans-serif;
}

/* Grilla - Letra de los Header (Success)*/
.sapMObjStatusSuccess .sapMObjStatusText, .sapMObjStatus .sapMObjStatusIcon {
	text-shadow: none;
	/*font-weight: bold;*/
	font-family: Arial,Helvetica,sans-serif;

}

/* Grilla - Letra de los Header (Error)*/
.sapMObjStatusError .sapMObjStatusText, .sapMObjStatus .sapMObjStatusIcon {
	text-shadow: none;
	/*font-weight: bold;*/
	font-family: Arial,Helvetica,sans-serif;
}

/* Grilla - Letra de los Header usando label */
.sapUiTableCell .sapMLabel, .sapUiTableColCell>.sapMLabel {
	color: #333333;
	background-color: #73d0f5;
	background-color: #EEEEEE;
	font-weight: bold;
	font-family: Arial,Helvetica,sans-serif;
}

/*Que se vea todo el texto de los iconos*/
.sapMITBFilter .sapMITBText{
	overflow: visible;
}

/*Primer icono sobreescribir padding*/
.sapMITBFilter:first-child {
	 padding-left: 1.5rem;
}

/*Espacio entre iconos filter*/
.sapMITBFilter{
	padding-right: 1.5rem;
	padding-left: 1.5rem;
}

/* Grilla - Columna final donde van los botones */
.sapUiTableRAct:not(.sapUiTableVScr) .sapUiTableRowActionHeader {
	width: 4rem;
	margin-right: 0;
	background-color: #D6C1DB;
	background-color: #EEEEEE;
	background-color: white;
}

/* Grilla - Celdas */
.sapUiTableCell .sapMText {
	vertical-align: middle;
	font-size: 12px;
	font-family: Arial,Helvetica,sans-serif;
}

/* Grilla - Columnas que han sido fijadas */
.sapUiTableCtrlScrFixed {
	float: left;
	max-width: 100%;
	overflow: hidden;
	background-color: #FEF0DB;
	background-color: #FAFAFA;
	background-color:#F0F0F0;
}

/* Input - PlaceHolder */
.sapMInputBaseInner:not(.sapMInputBaseReadonlyInner), .sapMInputBaseInner:not(.sapMInputBaseReadonlyInner) {
	outline: none;
	font-family: sans-serif;
}

/* Input - PlaceHolder (Buscar)*/
.sapMSFI[type='search'] {
	font-size: 0.875rem;
	font-family: sans-serif;
}

/* Tab - Container */
html .sapMITBScrollContainer, html:not(.sapUiMedia-Std-Phone) .sapMSplitContainer:not(.sapMSplitContainerHideMode) .sapMITB .sapMITBScrollContainer {
	margin: 0 1.75rem;
	font-family: Arial,Helvetica,sans-serif;
}

/* Tab - Seleccion de Tab */
.sapMITBSelected .sapMITBFilterPositive {
	background: #2b7d2b;
	color: #ffffff;
	background-color: #6c217f;
	background-color: #7E3C8F;
}

/* Tab - Positive */
.sapMITBFilterPositive {

	border-color: #CCB2D2;
	color: #CCB2D2;
	border-color: #7D398D;
	color: #7E3C8F;
}

/* Botones  - Color */
:not(.sapMBtnDisabled):not(.sapMSBActive)>span.sapMBtnInner.sapMBtnAccept:not(.sapMBtnActive):not(.sapMToggleBtnPressed)>.sapMBtnIcon {
	color: #2b7d2b;
	color: #7E3C8F;
}

