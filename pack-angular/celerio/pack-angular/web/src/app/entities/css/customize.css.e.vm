/* Container - Cabecera */
.sapMIBar.sapMHeader-CTX {
	background-color: white;
}

/* Container - Footer Pantalla Busqueda*/
.sapContrast .sapMIBar.sapMFooter-CTX, html.sap-desktop .sapContrast.sapMIBar.sapMFooter-CTX {
	background-color: #2f3c48;
	border-top: 1px solid #475b6d;
}

/* Container - Cabecera Mantenimientos */
.sapUxAPObjectPageHeaderIdentifier {
	padding-left: 3rem;
	background-color: aquamarine;
	background-color: white;
	background-color: #D6C1DB;
}

/* Container - Cabecera Mantenimientos  - Letra*/
.sapUxAPObjectPageHeaderTitleTextWrappable {
	white-space: normal;
	color: aliceblue;
}

/* Container - Cabecera Mantenimientos*/
.sapUiSizeCompact .sapUxAPObjectPageHeaderIdentifier {
    min-height: 2.5625rem;
    background-color: darkgoldenrod;
    background-color: #D6C1DB;
}

.sapMITH {
    box-shadow: inset 0 -0.25rem 0 -0.125rem #ebebeb;
    background-color: #fff;
    border-bottom: .125rem solid #fff;
    background-color: #EFF4F9;
}